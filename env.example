# ===========================================
# RACA FORTE BACKEND - VARIÁVEIS DE AMBIENTE
# ===========================================

# Database Configuration
DATABASE_URL="mysql://racaforte_user:racaforte_password@mysql:3306/racaforte_db"

# Server Configuration
HOST=0.0.0.0
PORT=3333
NODE_ENV=production

# AI Services
OPENAI_API_KEY=your-openai-api-key-here
GEMINI_API_KEY=your-gemini-api-key-here

# Unwrangle Service (opcional)
UNWRANGLE_API_KEY=your-unwrangle-api-key

# Hetzner S3 Storage
HETZNER_S3_ENDPOINT=https://your-hetzner-endpoint.com
HETZNER_S3_BUCKET=your-bucket-name
HETZNER_ACCESS_KEY=your-hetzner-access-key
HETZNER_SECRET_KEY=your-hetzner-secret-key

# Remove.bg API (para remoção de fundo de imagens)
REMOVEBG_API_KEY=your-removebg-api-key

# MySQL Database (usado apenas para docker-compose)
MYSQL_ROOT_PASSWORD=root_password
MYSQL_DATABASE=racaforte_db
MYSQL_USER=racaforte_user
MYSQL_PASSWORD=racaforte_password
